{
  "manifest_version": 2,

  "name": "Project Manager K Extension",
  "description": "This extension will display project deadlines and more.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "chrome_url_overrides" : {
    "newtab": "page.html"
  }
}