{
  "manifest_version": 2,

  "name": "Project Manager K Extension",
  "description": "This extension will display project deadlines and more.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
//    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "page.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}